@{
    ViewBag.Title = "Index";
    Layout = "IndexLayout.cshtml";
}

<textarea rows="4" cols="50" id="textMessage"></textarea>
<input type="text" id="textInput">
<input type="button" id="sendTextButton" value="Send Text">
<input type="text" id="userName">
<input type="button" id="updateUserNameButton" value="Update Username">
Session Id <input type="text" id="sessionId">
<input type="button" id="createSessionButton" value="Create Session">
<input type="button" id="joinSessionButton" value="Join Session">
<input type="button" id="endSessionButton" value="End Session">
<input type="button" id="rateSessionButton" value="Rate Session">
Event index <input type="text" id="eventIndex">
<input type="button" id="syncSessionButton" value="Sync Session">
<input type="button" id="stopConnection" value="Disconnect">
